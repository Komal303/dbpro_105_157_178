@model IEnumerable<SmartSchoolSystem.Models.TimeTableDetailedViewModels>

@{
    ViewBag.Title = "DetailedTimeTable";
}

<h2>DetailedTimeTable</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.dayname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lecture1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lecture2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lecture3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lecture4)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lecture5)
        </th>
       
    </tr>
@if (Model!=null)
{
foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.dayname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lecture1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lecture2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lecture3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lecture4)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lecture5)
        </td>
       
    </tr>
}
}
</table>
